                    {
                        "id": "strengthAttribute",
                        "label": "Strength Attribute",
                        "description": "Determines Carry Weight Limit and ability to apply Grapple, Knock Out, and Stagger",
                        "inputType": "slider",
                        "min": 0,
                        "max": 40,
                        "costPerPoint": {
                            "Attribute Points": 1,
                            "Strength": -1
                        }
