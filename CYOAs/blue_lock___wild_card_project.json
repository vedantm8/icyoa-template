            "cost": {},
            "id": "egoArchetypeMaverick",
            "image": "https://external-content.duckduckgo.com/iu/?u=https%3A%2F%2Fi.pinimg.com%2F736x%2Fa0%2F8b%2F32%2Fa08b32cb003b37db13c46296c430daf1.jpg&f=1&nofb=1&ipt=1ee2c041b8da57c62df3723a374386fc19ca457eb5327d1e9eb3f276a885436c",
